<% if @photographer %>
  <p>
    <strong>ユーザー名</strong>
    <%= @photographer.name %>
  </p>
  <p>
    <strong>e-mail</strong>
    <%= @photographer.email %>
  </p>

  <%= link_to "編集する", edit_photographer_registration_path, class: "btn btn-lg btn-primary" %>

<% else %>
  <p>ユーザーが存在しません</p>
<% end %>
